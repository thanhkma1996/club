  // Internal libraries
  // Fonts
  
  {% if settings.font_body contains 'Google_' %}
    $font-body: {{ settings.font_body | replace: 'Google_', '' | split: ':' | first | replace: '+', ' ' }};
  {% else %}
    $font-body: {{ settings.font_body }};
  {% endif %}
  
  {% if settings.font_title contains 'Google_' %}
    $font-title: {{ settings.font_title | replace: 'Google_', '' | split: ':' | first | replace: '+', ' ' }};
  {% else %}
    $font-title: {{ settings.font_title }};
  {% endif %}
  
  {% if settings.font_label contains 'Google_' %}
    $font-label: {{ settings.font_label | replace: 'Google_', '' | split: ':' | first | replace: '+', ' ' }};
  {% else %}
    $font-label: {{ settings.font_label }};
  {% endif %}
  
  // Main colors
  $color-text: {{ settings.color_text }};
  $color-title-text: {{ settings.color_title_text }};
  $color-accent: {{ settings.color_accent }};
  $color-button-text: {{ settings.color_button_text }};
  $color-background: #f7f7f7;
  
  // Secondary accent
  $color-secondary-text: $color-title-text;
  $color-secondary-background: rgba($color-title-text, 0.08);
